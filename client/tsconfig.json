{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@app/*": ["./app/*"],
      "@components/*": ["./components/*"],
      "@lib/*": ["./app/lib/*"],
      "@constants/*": ["./constants/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/groups/_layout.tsx"  ]
}
